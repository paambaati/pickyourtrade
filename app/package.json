{
  "name": "pickyourtrade",
  "version": "1.0.0",
  "private": true,
  "description": "PickYourTrade Webapp",
  "license": "UNLICENSED",
  "author": "GP <me@httgp.com>",
  "homepage": "https://github.com/paambaati/pickyourtrade",
  "bugs": {
    "url": "https://github.com/paambaati/pickyourtrade/issues",
    "email": "me@httgp.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paambaati/pickyourtrade"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest --verbose",
    "test:update-snapshots": "jest --updateSnapshot --verbose"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "buefy": "^0.7.7",
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "flush-promises": "^1.0.2",
    "jest": "^24.8.0",
    "jest-serializer-vue": "^2.0.2",
    "sinon": "^7.3.2",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10"
  },
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup/vueSetup.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "text-summary"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
