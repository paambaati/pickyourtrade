{
  "extends": "tslint-config-airbnb",
  "rules": {
    "align": [
      true,
      "parameters"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "strict-boolean-expressions": false,
    "max-line-length": [
      true,
      180
    ],
    "ter-indent": 4,
    "ter-arrow-parens": false,
    "object-shorthand-properties-first": false,
    "import-name": false
  }
}
